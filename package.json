{
  "name": "@cosmostation/cosmosjs",
  "licenses": [],
  "version": "0.9.0",
  "description": "A JavasSript Open Source Library for Cosmos Network, IRISnet, Kava, Band Protocol, Starname, Secret Network, Akash, and Certik. It supports for protobuf signing",
  "main": "src/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosmostation/cosmosjs.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "cosmosjs",
    "cosmos",
    "tendermint",
    "javascript",
    "atom",
    "cosmostation",
    "iris",
    "kava",
    "band",
    "iov",
    "starname",
    "secret",
    "scrt",
    "akash",
    "akt",
    "certik",
    "ctk",
    "stargate",
    "ibc",
    "protobuf"
  ],
  "scripts": {
    "dev": "node --es-module-specifier-resolution=node example/stargate-final.js",
    "send": "node --es-module-specifier-resolution=node send.js",
    "test": "mocha --es-module-specifier-resolution=node"
  },
  "author": "Cosmostation",
  "license": "MIT",
  "dependencies": {
    "bech32": "^1.1.3",
    "bip32": "^2.0.5",
    "bip39": "^3.0.3",
    "bitcoinjs-lib": "^4.0.2",
    "google-protobuf": "^3.14.0",
    "node-fetch": "^2.6.1",
    "protobufjs": "^6.10.2",
    "request": "^2.88.2",
    "secp256k1": "^3.6.2"
  },
  "devDependencies": {
    "mocha": "^7.1.1"
